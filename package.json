{
  "name": "nest-prisma-crud",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/main.ts",
    "start:dev": "npx prisma generate && npx prisma migrate dev --name init && ts-node-dev --respawn src/main.ts",
    "test": "npx jest",
    "test:e2e": "node test-expense-endpoints.js",
    "test:all": "npm run test && npm run test:e2e",
    "seed": "npx prisma db seed",
    "migrate": "npx prisma migrate dev",
    "migrate:prod": "npx prisma migrate deploy",
    "db:reset": "npx prisma migrate reset && npm run seed",
    "build": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/common": "^11.1.4",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.4",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.4",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.12.0",
    "@types/bcrypt": "^5.0.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "node-fetch": "^2.7.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^6.12.0",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.4",
    "@types/jest": "^30.0.0",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
